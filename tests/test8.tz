parameter (list bool) ;
storage (pair bool (pair nat int)) ;
code { DUP ; CAR ; DIP { CDR }; DIP {DUP ; CAR ; DIP { CDR }; DIP {DUP ; CAR ; DIP { CDR }} } ;
       # PUSH int 1; PUSH nat 0; PUSH bool True;
       # DUUUUP; DIIIIP { DROP } ;
       ITER {
              AND; DUP;
              IF { DIP 2 { PUSH int 1; ADD } }
                 { DIP 2 { PUSH int -1; ADD } }
            } ;
       DIP { PAIR }; PAIR; NIL operation; PAIR
       }
